{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
  {
    "src": "/(src/api|api-page)/.*",
    "status": 403,
    "dest": "/api-page/403.html"
  },
  {
    "src": "/(.*)",
    "dest": "/index.js"
  }
],
  "env": {
    "NODE_ENV": "production"
  }
}
